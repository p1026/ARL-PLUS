 :true-label=\"true\" :false-label=\"false\">Oneforall结果文件</el-checkbox>\n        </el-col>\n        <el-col :span=\"6\">\n          <el-checkbox v-model=\"arltmp\" :true-label=\"true\" :false-label=\"false\">nuclei、xray缓存</el-checkbox>\n        </el-col>\n        <el-col :span=\"6\">\n          <el-checkbox v-model=\"pfztmp\" :true-label=\"true\" :false-label=\"false\">PFZ未授权扫描缓存</el-checkbox>\n        </el-col>\n        <el-col :span=\"6\">\n          <el-checkbox v-model=\"pfzres\" :true-label=\"true\" :false-label=\"false\">PFZ未授权结果缓存</el-checkbox>\n        </el-col>\n      </el-row>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { clear } from '@/api/article'\n\nexport default {\n  name: 'ErrorLog',\n  data() {\n    return {\n      onetmp: false,\n      arltmp: false,\n      pfztmp: false,\n      pfzres: false,\n      dialogTableVisible: false\n    }\n  },\n  computed: {\n    errorLogs() {\n      return this.$store.getters.errorLogs\n    }\n  },\n  methods: {\n    clearAll() {\n      this.dialogTableVisible = false\n      this.$confirm('确认清除后台缓存吗? 确认所有任务完成后清除，否则会造成数据丢失和程序中断', '提示', {\n        type: 'warning'\n      }).then(() => {\n        const data = {\n          'onetmp': this.onetmp,\n          'arltmp': this.arltmp,\n          'pfztmp': this.pfztmp,\n          'pfzres': this.pfzres\n        }\n        clear(data).then(response => {\n          if (response.code === 200) {\n            this.$notify({\n              title: '成功',\n              message: '清除成功',\n              type: 'success',\n              duration: 2000\n            })\n            this.$router.push('/rule/rule-info')\n          }\n        }).catch(error => {\n        // 处理请求异常\n          this.$notify({\n            title: '失败',\n            message: error.message,\n            type: 'error',\n            duration: 2000\n          })\n        })\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.message-title {\n  font-size: 16px;\n  color: #333;\n  font-weight: bold;\n  padding-right: 8px;\n}\n</style>\n"]}]}